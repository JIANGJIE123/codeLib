/* 头像昵称  */
.wd .top{background-color: #Da251D;height: 330rpx;}
.top{position: relative;}
.header{position: absolute;left: 50%;top:50%;transform:translate(-50%,-50%);text-align: center;}
.headImg{width: 150rpx;height: 150rpx;border-radius:50%;overflow:hidden;display: block}
.headNick{text-align: center;font-size: 35rpx;color: #f0f0f0;}


